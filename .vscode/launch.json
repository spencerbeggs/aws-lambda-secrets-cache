{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "debug",
        "type": "node",
        "request": "launch",
        "program": "server",
        "runtimeExecutable": "node",
        "runtimeArgs": [
            "--inspect",
            "--experimental-vm-modules",
            "--no-warnings",
            "node_modules/.bin/jest",
            "--passWithNoTests",
            "--config",
            "./.jestrc.ts"
        ],
        "env": {
            "APP_ENV": "local"
        },
        "autoAttachChildProcesses": true,
        "outFiles": ["${workspaceFolder}/.next/**/*.js"]
      }
    ]
}
  